    "start:local": "./node_modules/.bin/env-cmd -f ./.env.local react-app-rewired start",
    "start:uat": "./node_modules/.bin/env-cmd -f ./.env.uat react-app-rewired start",
    "start:staging": "./node_modules/.bin/env-cmd -f ./.env.staging react-app-rewired start",
    "start:prod": "./node_modules/.bin/env-cmd -f ./.env.prod react-app-rewired start",
    "build": "react-app-rewired --max_old_space_size=2510 build",
    "build:local": "./node_modules/.bin/env-cmd -f ./.env.local react-app-rewired --max_old_space_size=2510 build",
    "build:uat": "./node_modules/.bin/env-cmd -f ./.env.uat react-app-rewired --max_old_space_size=2510 build",
    "build:staging": "./node_modules/.bin/env-cmd -f ./.env.staging react-app-rewired --max_old_space_size=2510 build",
