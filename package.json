    "test": "react-app-rewired test",
    "test:local": "./node_modules/.bin/env-cmd -f ./.env.local react-app-rewired test",
    "test:uat": "./node_modules/.bin/env-cmd -f ./.env.uat react-app-rewired test",
    "test:staging": "./node_modules/.bin/env-cmd -f ./.env.staging react-app-rewired test",
